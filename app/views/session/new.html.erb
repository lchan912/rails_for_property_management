<div class='property'>

  <%= form_for :session, url: login_path do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username, placeholder: 'username' %>
    <%= f.label :password %>
    <%= f.text_field :password, placeholder: 'password', type: 'password' %>
    <%= f.submit "Sign In" %>
  <% end %>

</div>
